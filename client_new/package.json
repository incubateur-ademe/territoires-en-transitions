{
  "name": "new.territoiresentransitions.fr",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run build:tailwindcss && sapper build",
    "export": "npm run build:tailwindcss && sapper export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "watch:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "build:tailwindcss": "cross-env NODE_ENV=production postcss static/tailwind.css -o static/main.css"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "@types/uuid": "8.3.0",
    "autoprefixer": "10.2.4",
    "chai": "4.3.0",
    "cross-env": "7.0.3",
    "file-loader": "^6.0.0",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.3.0",
    "tailwindcss": "2.0.3",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "uuid": "3.4.0",
    "webpack": "^4.46.0",
    "webpack-modules": "^1.0.0"
  }
}
