    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.e2e-spec.ts",
  ],
  "exclude": [
  ],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    }
  ]
}

