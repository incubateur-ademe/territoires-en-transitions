{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.app.json",
    "builder": "swc",
    "assets": [
      { "include": "**/*.csv", "outDir": "dist" },
      { "include": "**/*.xlsx", "outDir": "dist" }
    ]
  }
}
