{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "command": "nest start --watch",
      "// debug": "nest start --debug --watch",
      "options": {
        "cwd": "apps/backend"
      }
    },
    "build": {
      "command": "nest build",
      "options": {
        "cwd": "apps/backend"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/backend"
      }
    }
  }
}
