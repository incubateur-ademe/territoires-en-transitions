{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    }
  ],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@/domain/**"]
      }
    ],
    "@typescript-eslint/no-non-null-assertion": "error",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "ignoreRestSiblings": true }
    ]
  }
}
